{
  "name": "heyblackmagic/foundation-ddev",
  "type": "project",
  "description": "Craft CMS - DDEV scafold",
  "license": "MIT",
  "minimum-stability": "beta",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^5.0.0",
    "craftcms/ckeditor": "^4.0.3",
    "vlucas/phpdotenv": "^5.6.0",
    "nystudio107/craft-vite": "^5.0.0-beta.3",
    "verbb/hyper": "^2.0.0-beta.7",
    "solspace/craft-freeform": "^5.1.9",
    "nystudio107/craft-seomatic": "^5.0.0-beta.8",
    "verbb/navigation" : "^3.0.0-beta.2"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
