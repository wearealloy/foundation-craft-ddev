{
  "name": "heyblackmagic/foundation-ddev",
  "type": "project",
  "description": "Craft CMS - DDEV scafold",
  "license": "MIT",
  "require": {
    "craftcms/cms": "^4.4.0",
    "craftcms/ckeditor": "3.8.1",
    "vlucas/phpdotenv": "^5.4.0",
    "nystudio107/craft-vite": "4.0.9",
    "spicyweb/craft-quick-field": "^2.0.7",
    "verbb/hyper": "^1.1.21",
    "solspace/craft-freeform": "^5.0.0",
    "nystudio107/craft-seomatic": "^4.0.44",
    "verbb/navigation" : "^2.0.26"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}

